{
  "name": "electron-timer",
  "productName": "Ryan's Cool Timer",
  "configuration": {
    "icon": "./public/favicon.png"
  },
  "icon": "./public/favicon.png",
  "version": "1.0.0",
  "description": "A timer application for working, presenting, streaming, or anything else.",
  "main": "src/index.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "concurrently \"npm:svelte-dev\" \"electron-forge start\"",
    "electron": "run-s build pure-electron",
    "pure-electron": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "svelte-build": "rollup -c",
    "svelte-dev": "rollup -c -w",
    "svelte-start": "sirv public",
    "electron-dev": "concurrently \"yarn run dev\" \"yarn run electron\"",
    "preelectron-pack": "yarn run build",
    "electron-pack": "electron-builder",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=public/build/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . \"Ryan's Cool Timer\" --overwrite --asar --platform=win32 --arch=ia32 --icon=public/build/icon.ico --prune=true --out=release-builds"
  },
  "keywords": [],
  "author": {
    "name": "Ryan McKay",
    "email": "44735056+TheDitis@users.noreply.github.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-zip"
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-serve": "^1.1.0",
    "sound-play": "^1.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fouita/tooltip": "^0.0.1",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "color": "^3.1.3",
    "concurrently": "^6.0.0",
    "electron": "12.0.1",
    "electron-builder": "^22.10.5",
    "electron-reload": "^1.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "fontawesome": "^5.6.3",
    "lodash": "^4.17.21",
    "luxon": "^1.26.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "play-sound": "^1.1.3",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-multi-input": "^1.2.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv": "^1.0.11",
    "svelte": "^3.0.0",
    "svelte-fa": "^2.2.0",
    "svelte-loading-spinners": "^0.1.4",
    "svelte-preprocess": "^4.6.9",
    "svelte-watch-resize": "^1.0.3",
    "svg-partial-circle": "^1.0.0"
  }
}
